<import src="/tmpl/loading/loading.wxml" />
<import src="/tmpl/list/list.wxml" />

<view class="index">
  <view class="flex flex-y-center header">
    <view class="flex-1 line-1 location" data-tag="location" bindtap="jump">
      <text class="city">{{city}}</text>
      <text class="iconfont icon-xiajiantou"></text>
    </view>
    <view class="flex-0 search" data-tag="search" bindtap="jump">
      <text class="flex-0 iconfont icon-search"></text>
      <text class="text">请输入医院名称</text>
    </view>
  </view>
  <view class="container">
    <view class=""></view>
    <template is="hosp" data="{{hospList}}" />    
  </view>
</view>