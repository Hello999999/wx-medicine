page {
    background-color: var(--bgColor);
    --bgColor: #f5f5f5;
    --colorOne: #666;
    --colorTwo: #F7EFD2;
    --fontSizeOne: 30rpx;
    --fontSizeTwo: 34rpx;
    --linkColor: rgb(70, 145, 214);
    --borderColorOne: #f5f5f5;
    --borderColorTwo: #ddd;
    --backgroundColorOne: royalblue;
    --colorOne: #aaa;
}

.flex {
    display: flex;
}

.flex-x-center {
    justify-content: center;
}

.flex-y-center {
    align-items: center;
}

.flex-0 {
    flex-grow: 0;
    flex-shrink: 0;
}

.flex-1 {
    flex-grow: 1;
    flex-shrink: 1;
}

.line-1 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.line-2 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.no-button {
    /* position: unset; */
    display: block;
    margin: inherit;
    padding: inherit;
    box-sizing: inherit;
    font-size: inherit;
    text-align: inherit;
    /* text-decoration: unset; */
    line-height: inherit;
    border-radius: inherit;
    /* -webkit-tap-highlight-color: unset; */
    overflow: inherit;
    color: inherit;
    background-color: inherit;
}

.no-button::after {
    border: 0;
}

.bg-white {
    background-color: #fff;
}

.n-b {
    border-bottom: 0 !important;
}